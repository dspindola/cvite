{
  "name": "cvite",
  "type": "module",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "prepare": "bun build --compile node_modules/civetman/dist/index.js --outfile ./node_modules/.bin/civetman",
    "civet": "bun civetman --noGit --noVscode",
    "dev": "bun civet -- dev",
    "build": "bun civet -- build",
    "clean": "rm -rf \"rm src/**/{*.tsx,*.ts,*.js}\""
  },
  "devDependencies": {
    "@types/bun": "1.2.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.22.0",
    "eslint-plugin-civet": "^0.0.6"
  },
  "peerDependencies": {
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@danielx/civet": "0.9.7",
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/eden": "^1.2.0",
    "better-auth": "^1.2.3",
    "civetman": "^0.0.4",
    "mlly": "^1.7.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ungh": "^0.0.0"
  },
  "civetConfig": {
    "objectIs": true,
    "implicitReturns": false,
    "tab": 2
  }
}
